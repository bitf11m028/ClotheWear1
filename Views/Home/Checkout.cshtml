@{
    ViewBag.Title = "Checkout";
}
<table border="1">
    <tr>
        <th>
            ID
        </th>
        <th>
            Title
        </th>
        <th>
            Price
        </th>
        <th>
            Description
        </th>
        <th>
            Category
        </th>
    </tr>

@foreach (var s in Model)
{
    <tr>
        <td>
            @s.Id
        </td>
        <td>
            @s.Title
        </td>
        <td>
            @s.Price
        </td>
        <td>
            @s.Description
        </td>
        <td>
            @s.Category
        </td>
    </tr>
}
</table>